<%= form_for @student do |f| %>
  <label> Student Form </label><br>
  <label> Enter First Name: </label>
  <%= f.text_field :first_name, id: 'first_name' %><br>
  <label> Enter Last Name: </label>
  <%= f.text_field :last_name, id: 'last_name' %><br>
  <%= f.submit :submit, id: "Submit Student" %>

<% end %>
